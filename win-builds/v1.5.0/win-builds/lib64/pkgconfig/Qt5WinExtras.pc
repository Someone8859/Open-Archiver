prefix=/opt/windows_64
exec_prefix=${prefix}
libdir=${prefix}/lib64
includedir=${prefix}/include/qt5


Name: Qt5 WinExtras
Description: Qt WinExtras module
Version: 5.3.1
Libs: -L${libdir} -lQt5WinExtrasd 
Libs.private: -lglu32 -lopengl32 -lgdi32 -luser32 -L/opt/windows_64/lib64 -lQt5Guid -lQt5Cored -lole32 -lshlwapi -lshell32  
Cflags: -I${includedir}/QtWinExtras -I${includedir}
Requires: Qt5Core Qt5Gui

